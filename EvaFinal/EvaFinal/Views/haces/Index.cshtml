            <td>
                @Html.DisplayFor(modelItem => item.Nota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.alumnos.NumMatricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.examen.ExamenId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.hacerid">Edit</a> |
