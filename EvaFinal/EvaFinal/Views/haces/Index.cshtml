@model IEnumerable<EvaFinal.Models.hace>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nota)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.alumnos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.examen)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
